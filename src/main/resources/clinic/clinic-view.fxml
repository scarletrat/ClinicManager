<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.ClinicManagerController">
    <center>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="Schedule/Cancel">
                    <content>
                        <AnchorPane minHeight="5.0" minWidth="5.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <GridPane layoutX="16.0" layoutY="30.0" prefHeight="289.0" prefWidth="539.0" style="-fx-border-color: blue;" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="227.0" minWidth="10.0" prefWidth="150.6" />
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="342.6" minWidth="10.0" prefWidth="262.20000000000005" />
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="225.2" minWidth="10.0" prefWidth="160.79999999999995" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="103.4" minHeight="10.0" prefHeight="103.4" vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="17.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label alignment="CENTER" contentDisplay="CENTER" text="Appointment Date" GridPane.halignment="RIGHT">
                                 <GridPane.margin>
                                    <Insets right="10.0" top="10.0" />
                                 </GridPane.margin></Label>
                                        <DatePicker prefHeight="25.0" prefWidth="170.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
                                 <GridPane.margin>
                                    <Insets top="10.0" />
                                 </GridPane.margin></DatePicker>
                                        <Label contentDisplay="RIGHT" text="Patient" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
                                 <GridPane.margin>
                                    <Insets right="10.0" />
                                 </GridPane.margin></Label>
                                        <VBox prefHeight="117.0" prefWidth="319.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                            <children>
                                                <TextField prefHeight="25.0" prefWidth="292.0" promptText="first name">
                                       <VBox.margin>
                                          <Insets top="10.0" />
                                       </VBox.margin></TextField>
                                                <TextField promptText="last name">
                                       <VBox.margin>
                                          <Insets top="5.0" />
                                       </VBox.margin></TextField>
                                                <DatePicker prefHeight="25.0" prefWidth="170.0" promptText="date of birth">
                                       <VBox.margin>
                                          <Insets top="5.0" />
                                       </VBox.margin></DatePicker>
                                            </children></VBox>
                                        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                 <children>
                                    <RadioButton fx:id="officeApt" mnemonicParsing="false" onAction="#imagingSelected" text="Office Visit">
                                       <HBox.margin>
                                          <Insets top="5.0" />
                                       </HBox.margin>
                                       <toggleGroup>
                                          <ToggleGroup fx:id="apptType" />
                                       </toggleGroup>
                                    </RadioButton>
                                    <RadioButton fx:id="imagingApt" mnemonicParsing="false" onAction="#imagingSelected" text="Imaging Service" toggleGroup="$apptType">
                                       <HBox.margin>
                                          <Insets left="20.0" top="5.0" />
                                       </HBox.margin>
                                    </RadioButton>
                                 </children></HBox>
                                        <HBox prefHeight="72.0" prefWidth="299.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                                 <children>
                                    <ComboBox fx:id="cmb_timeslot" prefWidth="110.0" promptText="Timeslot">
                                       <HBox.margin>
                                          <Insets right="10.0" />
                                       </HBox.margin></ComboBox>
                                    <ComboBox fx:id="cmb_provider" maxWidth="-Infinity" prefWidth="110.0" promptText="Providers">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </ComboBox>
                                 </children></HBox>
                                        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                                 <children>
                                    <Button mnemonicParsing="false" prefWidth="110.0" text="Schedule">
                                       <HBox.margin>
                                          <Insets right="10.0" />
                                       </HBox.margin>
                                    </Button>
                                    <Button mnemonicParsing="false" prefWidth="110.0" text="Cancel">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </Button>
                                 </children></HBox>
                              <Button fx:id="loadButton" mnemonicParsing="false" onAction="#loadProviders" prefWidth="100.0" text="Load Providers" GridPane.columnIndex="2" GridPane.rowIndex="3" GridPane.valignment="TOP">
                                 <GridPane.margin>
                                    <Insets left="10.0" />
                                 </GridPane.margin>
                              </Button>
                              <Button mnemonicParsing="false" prefWidth="100.0" text="clear" GridPane.columnIndex="2" GridPane.rowIndex="4" GridPane.valignment="TOP">
                                 <GridPane.margin>
                                    <Insets left="10.0" />
                                 </GridPane.margin>
                              </Button>
                                    </children>
                                </GridPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Reschedule">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
                <Tab text="Clinic Locations">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="tbl_location" layoutX="25.0" layoutY="14.0" prefHeight="307.0" prefWidth="534.0">
                          <columns>
                            <TableColumn prefWidth="75.0" text="City" />
                            <TableColumn fx:id="col_county" prefWidth="75.0" text="County" />
                              <TableColumn fx:id="col_zip" prefWidth="75.0" text="Zip" />
                          </columns>
                        </TableView>
                     </children></AnchorPane>
                    </content>
                </Tab>
            </tabs>
         <BorderPane.margin>
            <Insets left="10.0" right="10.0" />
         </BorderPane.margin>
        </TabPane>
    </center>
   <bottom>
      <TextArea fx:id="outputArea" prefHeight="200.0" prefWidth="200.0" style="-fx-border-color: red;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </BorderPane.margin>
      </TextArea>
   </bottom>
</BorderPane>
